{
  "name": "@getbizi/client",
  "version": "0.3.0",
  "description": "Client library for the bizi service",
  "license": "MIT",
  "repository": "https://github.com/ieedan/bizi",
  "homepage": "https://getbizi.dev",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "files": [
    "dist/**/*",
    "!dist/**/*.map"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "check": "tsc --noEmit",
    "generate": "openapi-typescript http://127.0.0.1:7436/openapi.json -o ./src/api.ts",
    "pack": "bun run build && bun pm pack"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "openapi-fetch": "^0.17.0",
    "openapi-typescript": "^7.13.0",
    "typescript": "^5.9.3",
    "tsdown": "0.19.0-beta.3"
  }
}

